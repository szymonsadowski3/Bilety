<div class="container mt-5">

<h2>Lista wydarze≈Ñ</h2>

<% for event in Event.all %>
  <table class="table">
    <thead>
    <tr>
      <th>Artist</th>
      <th>description</th>
      <th>price_low</th>
      <th>price_high</th>
      <th>event_date</th>
      <th>event_id</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
      <tr>
        <td><%= event.artist %></td>
        <td><%= event.description %></td>
        <td><%= event.price_low %></td>
        <td><%= event.price_high %></td>
        <td><%= event.event_date %></td>
        <td><%= event.id %></td>
      </tr>
    </tbody>
  </table>
<% end %>

<h1>New Ticket</h1>

<%= render 'form', ticket: @ticket %>

  <hr>

<button class="btn btn-default">
<%= link_to 'Back', tickets_path %>
</button>
</div>